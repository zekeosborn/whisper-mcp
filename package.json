{
    "name": "whisper-mcp",
    "version": "0.0.1",
    "private": true,
    "workspaces": ["packages/*"],
    "scripts": {
      "build": "npm run -w packages/mcp build",
      "start": "npm run -w packages/mcp start",
      "backend:build": "npm run -w packages/backend build",
      "backend:start": "npm run -w packages/backend start",
      "backend:prisma:dev": "npm run -w packages/backend prisma:dev",
      "backend:prisma:deploy": "npm run -w packages/backend prisma:deploy"
    }
  }